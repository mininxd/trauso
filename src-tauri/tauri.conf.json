{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "trauso",
  "version": "1.0.2",
  "identifier": "com.trauso.cli",
  "build": {
    "beforeDevCommand": "npm run vite:dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run vite:build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {}
}
